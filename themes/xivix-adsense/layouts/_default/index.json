{{- $.Scratch.Add "index" slice -}}
{{- range .Site.RegularPages -}}
    {{- $.Scratch.Add "index" (dict "title" .Title "url" .Permalink "content" .Plain "summary" (.Summary | plainify) "date" (.Date.Format "2006-01-02") "categories" .Params.categories "tags" .Params.tags) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
